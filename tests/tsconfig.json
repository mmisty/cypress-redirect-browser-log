{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmit": true,
    "strict": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "isolatedModules": true,
    "types": ["node", "jest", "cypress", "jest-test-each"],
//    "baseUrl": "./",
//    "paths": {
//      "cy-local": ["../src"],
//      "cy-local/*": ["../src/*"]
//    }
  }
}
